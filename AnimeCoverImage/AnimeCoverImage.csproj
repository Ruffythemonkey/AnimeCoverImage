  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.11.60" />
    <PackageReference Include="Microsoft.Net.Http.Headers" Version="8.0.4" />
  </ItemGroup>

</Project>
